<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ResponseCache async="false" continueOnError="false" enabled="true" name="RC-CacheRespones">
    <DisplayName>RC-CacheRespones</DisplayName>
    <Properties/>
    <CacheKey>
        <KeyFragment ref="request.uri" type="string"/>
        <KeyFragment ref="request.content" type="string"/>
    </CacheKey>
    <Scope>Exclusive</Scope>
    <ExpirySettings>
        <ExpiryDate/>
        <TimeOfDay/>
        <TimeoutInSec ref="">10</TimeoutInSec>
    </ExpirySettings>
    <SkipCacheLookup>request.header.bypass-cache = "true"</SkipCacheLookup>
</ResponseCache>